<div class="container custom-container">
  <div class="row">
    <%= render 'to_do_items/to_do_item_list' %>
  </div>
  <div class="row buffer"></div>

  <div class="row">
    <% @lists.each do |list| %>
      <div class="col-md-5">
        <%= render 'list_with_items', list: list %>
      </div>
      <div class="col-md-2 separator"></div>
    <% end %>
  </div>
</div>