<div class="embed-container" style="width: 100%; min-height: 200px; flex: 1 1 auto;">
  <iframe class="embed" width="100%" height="100%"
          style="flex: 1 1 auto" src="https://workflowy.com/embed/<%= ENV['WORKFLOWY_ID']%>"></iframe>
</div>
<div class="container">
  <div class="row">
  <% @lists.each do |list| %>
  <div class="col-md-6">
    <%= render 'list_with_items', list: list %>
  </div>
  <% end %>
  </div>
</div>